import java.io.*;
import java.math.*;
import java.text.*;
import java.util.*;
import java.util.regex.*;


public class Solution { 
   public static void main(String[] args){ 
      Scanner in = new Scanner(System.in);  
      int n = in.nextInt(); 
      for(int i=0; i<n; i++){ 
        int grade = in.nextInt(); 
        if(grade>=38){
          if(grade%5>=3){  
              grade+=5-(grade%5);
          } 
        } 
         System.out.println(grade);
      }
   } 
}
